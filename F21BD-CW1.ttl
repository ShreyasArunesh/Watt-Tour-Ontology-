###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#AdultAttractions
:AdultAttractions rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                               owl:complementOf [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty :isAttractedBy ;
                                                                                  owl:someValuesFrom :KidsTourists
                                                                                ]
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :isAttractedBy ;
                                                               owl:someValuesFrom :AdultTourists
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf :Places ;
                  owl:disjointWith :KidsAttractions ;
                  rdfs:comment "The adult Attractions"@en ;
                  rdfs:label "AdultAttractions"@en .


###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#AdultTourists
:AdultTourists rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :isAttractedTo ;
                                     owl:someValuesFrom :Places
                                   ] ;
               rdfs:subClassOf :Tourists ;
               rdfs:comment "Adult Tourists"@en ;
               rdfs:label "AdultTourists"@en .


###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#Clubs
:Clubs rdf:type owl:Class ;
       rdfs:subClassOf :AdultAttractions ;
       owl:disjointWith :Temples ;
       rdfs:comment "The clubs"@en ;
       rdfs:label "Clubs"@en .


###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#Description
:Description rdf:type owl:Class ;
             rdfs:subClassOf :WattTour ;
             rdfs:comment "The description of the tour"@en ;
             rdfs:label "Description"@en .


###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#KidsAttractions
:KidsAttractions rdf:type owl:Class ;
                 rdfs:subClassOf :Places ;
                 rdfs:comment "The kids Attractions"@en ;
                 rdfs:label "KidsAttractions"@en .


###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#KidsTourists
:KidsTourists rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :isAttractedTo ;
                                    owl:allValuesFrom :KidsAttractions
                                  ] ;
              rdfs:subClassOf :Tourists ;
              rdfs:comment "Kids Tourists"@en ;
              rdfs:label "KidsTourists"@en .


###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#Museums
:Museums rdf:type owl:Class ;
         rdfs:subClassOf :KidsAttractions ;
         rdfs:comment "The Museums"@en ;
         rdfs:label "Museums"@en .


###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#Name
:Name rdf:type owl:Class ;
      rdfs:subClassOf :WattTour ;
      rdfs:comment "The Name of the tour"@en ;
      rdfs:label "Name"@en .


###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#OneDay
:OneDay rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :hasCities ;
                              owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                              owl:onClass <https://github.com/ShreyasArunesh/Watt-Tour-Ontology-/blob/main/F21BD-CW1.owl#City(wikidata:City)>
                            ] ;
        rdfs:subClassOf :TourPlans ;
        rdfs:comment "All one day plans are included"@en ;
        rdfs:label "One day tour plans" .


###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#Parks
:Parks rdf:type owl:Class ;
       rdfs:subClassOf :KidsAttractions ;
       owl:disjointWith :Temples ;
       rdfs:comment "The Parks"@en ;
       rdfs:label "Parks"@en .


###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#Person
:Person rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :canRegiesterFor ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :WattTour
                            ] ;
        rdfs:comment "The Person"@en ;
        rdfs:label "Person"@en .


###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#Places
:Places rdf:type owl:Class ;
        rdfs:comment "Places covered in the Tour"@en ;
