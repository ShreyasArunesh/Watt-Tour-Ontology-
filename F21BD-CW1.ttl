         rdfs:comment "The Temples"@en ;
         rdfs:label "Temples"@en .


###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#TourPlans
:TourPlans rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasCities ;
                                 owl:someValuesFrom <https://github.com/ShreyasArunesh/Watt-Tour-Ontology-/blob/main/F21BD-CW1.owl#City(wikidata:City)>
                               ] ;
           rdfs:comment "The tour Plans"@en ;
           rdfs:label "TourPlans"@en .


###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#Tourists
:Tourists rdf:type owl:Class ;
          rdfs:subClassOf :Person ;
          rdfs:comment "Tourists involved in the Tour"@en ;
          rdfs:label "Tourists"@en .


###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#TwoDay
:TwoDay rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasCities ;
                                                     owl:minQualifiedCardinality "4"^^xsd:nonNegativeInteger ;
                                                     owl:onClass <https://github.com/ShreyasArunesh/Watt-Tour-Ontology-/blob/main/F21BD-CW1.owl#City(wikidata:City)>
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasCities ;
                                                     owl:maxQualifiedCardinality "6"^^xsd:nonNegativeInteger ;
                                                     owl:onClass <https://github.com/ShreyasArunesh/Watt-Tour-Ontology-/blob/main/F21BD-CW1.owl#City(wikidata:City)>
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf :TourPlans ;
        rdfs:comment "All two day plans are included"@en ;
        rdfs:label "Two day tour plans" .


###  http://www.semanticweb.org/shreyasarunesh/ontologies/2022/0/untitled-ontology-27#WattTour
:WattTour rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                owl:onProperty :spansFor ;
                                                owl:someValuesFrom :OneDay
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :spansFor ;
                                                owl:someValuesFrom :TwoDay
                                              ]
                                            )
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :willHave ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :Person
                              ] ;
          rdfs:comment "The Name and description of the tour"@en ;
          rdfs:label "WatTour"@en .


